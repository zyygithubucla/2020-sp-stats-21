    "def arc(t, r, angle):\n",
    "    circumference = math.pi * 2 * r\n",
    "    arc_length = circumference * angle/360\n",
    "    n = int(arc_length / 3) + 1\n",
    "    step_size = arc_length / n\n",
    "    step_angle = angle / n\n",
    "    for i in range(n):\n",
    "        t.forward(step_size)\n",
    "        t.left(step_angle)"
